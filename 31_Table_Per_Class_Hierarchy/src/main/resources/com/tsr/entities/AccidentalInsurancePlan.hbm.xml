<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<subclass name="com.tsr.entities.AccidentalInsurancePlan" extends="com.tsr.entities.InsurancePlan" discriminator-value="AIP">
		<property name="coverageType" column="coverage_type" />
		<property name="disabilityCoveragePercentage"
			column="disability_coverage_percentage" />
	</subclass>
</hibernate-mapping>