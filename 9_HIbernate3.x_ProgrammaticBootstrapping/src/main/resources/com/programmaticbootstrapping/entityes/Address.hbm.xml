<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.programmaticbootstrapping.entityes.Address" table="address">
		<id name="addressNo" column="address_no"/>
		<property name="addressLine1" column="address_line1"/>
		<property name="addressLine2" column="address_line2"/>
		<property name="city"/>
		<property name="state"/>
		<property name="country"/>
		<property name="zip"/>
	</class>
</hibernate-mapping>