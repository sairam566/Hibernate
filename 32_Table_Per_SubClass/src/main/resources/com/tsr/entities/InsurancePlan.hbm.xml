<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.tsr.entities.InsurancePlan" table="insurance_plan">
		<id name="insurancePlanNo">
			<column name="insurance_Plan_No"/>
			<generator class="sequence"/>
		</id>
		<property name="planName" column="plan_name"/>
		<property name="description"/>
		<property name="taxSaver" column="tax_saver"/>
		<property name="minInsurredAmount" column="min_Insurred_Amount"/>
	</class>
</hibernate-mapping>